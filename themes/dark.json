            ],
            "settings": {
                "foreground": "#FC427B"
            }
        },
        {
            "name": "String",
            "scope": ["string"],
            "settings": {
                "foreground": "#b0ffa2"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "support.class",
                "support.orther.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types",
                "support.type.exception.python"
